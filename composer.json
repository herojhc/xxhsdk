{
  "name": "herojhc/xxh-sdk",
  "description": "xinxihua.com sdk.",
  "keywords": [
    "xinxihua",
    "api",
    "client",
    "oauth2",
    "laravel"
  ],
  "homepage": "https://www.xinxihua.com",
  "license": "MIT",
  "authors": [
    {
      "name": "herojhc",
      "email": "759242210@qq.com",
      "homepage": "https://www.xinxihua.com/"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "laravel/framework": "~5.5|~6.0|~7.0",
    "doctrine/dbal": "~2.6",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "XinXiHua\\": "src/XinXiHua/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "XinXiHua\\SDK\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "XinXiHua\\SDK\\XXHServiceProvider"
      ],
      "aliases": {
        "XXH": "XinXiHua\\SDK\\Facades\\XXH"
      }
    }
  }
}
